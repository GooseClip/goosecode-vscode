{
  "name": "goosecode",
  "displayName": "GooseCode",
  "description": "Your codebase minimap and more.",
  "version": "0.1.6",
  "publisher": "gooseclip",
  "engines": {
    "vscode": "^1.77.0"
  },
  "license": "MIT",
  "private": true,
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "goosecode",
    "minimap",
    "overlay",
    "code",
    "navigation",
    "snippet",
    "generation",
    "visualize"
  ],
  "preview": false,
  "homepage": "https://gooseclip.com/goosecode",
  "qna": "https://github.com/GooseClip/goosecode-issues/issues",
  "icon": "resources/icon_transparent.png",
  "galleryBanner": {
    "color": "#3B3F47",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/GooseClip/goosecode-issues/issues",
    "email": "support@gooseclip.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GooseClip/goosecode-vscode.git"
  },
  "pricing": "Free",
  "activationEvents": [
    "*"
  ],
  "extensionDependencies": [
    "vscode.git"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "goosecode.connection",
        "contents": "GooseCode extension is not running.\n[Click here](command:goosecode.enableAutoStart) if you want GooseCode to start automatically.\nOtherwise [start for this session](command:goosecode.start).\n[Open Settings](command:goosecode.openSettings)\n[Documentation](command:goosecode.openWalkthrough)",
        "when": "goosecode.started == false && goosecode.portConflict != true"
      },
      {
        "view": "goosecode.connection",
        "contents": "The configured port is already in use by another application or VSCode window.\n[Change Port for This Workspace](command:goosecode.changePortForWorkspace)\n[Open Settings](command:goosecode.openSettings)",
        "when": "goosecode.portConflict == true"
      },
      {
        "view": "goosecode.connection",
        "contents": "Creating a [GooseCode](https://goosecode.app) Connection:\nCheck or update connection details in [Settings](command:goosecode.openSettings).\nConnect via the GooseCode IDE controls pane.\nPlease view the walkthrough for detailed instructions.\n[Documentation](command:goosecode.openWalkthrough)",
        "when": "goosecode.started == true && goosecode.initialized == false"
      },
      {
        "view": "goosecode.codeSources",
        "contents": "Open a workspace to configure your code sources."
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "goosecode",
          "title": "GooseCode",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "goosecode": [
        {
          "id": "goosecode.connection",
          "name": "Connection"
        },
        {
          "id": "goosecode.codeSources",
          "name": "Code Sources"
        },
        {
          "id": "goosecode.activeSession",
          "name": "Active Session",
          "when": "goosecode.initialized == true"
        }
      ]
    },
    "keybindings": [
      {
        "command": "goosecode.generate",
        "key": "alt+ctrl+g",
        "mac": "alt+cmd+g",
        "when": "editorTextFocus"
      },
      {
        "command": "goosecode.snippet",
        "key": "alt+ctrl+s",
        "mac": "alt+ctrl+cmd+s",
        "when": "editorTextFocus"
      },
      {
        "command": "goosecode.snippetMinimized",
        "key": "alt+ctrl+m",
        "mac": "alt+ctrl+cmd+m",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "goosecode.openWalkthrough",
        "title": "View Walkthrough",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.openSettings",
        "title": "Open Settings",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.appcommand.minimap",
        "title": "Toggle Minimap",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.appcommand.overlay",
        "title": "Toggle Overlay",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.codeSources.refresh",
        "title": "Refresh Code Sources",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "goosecode.codeSources.enableCodeSource",
        "title": "Enable CodeSource",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/toggle_off.svg",
          "dark": "resources/dark/toggle_off.svg"
        }
      },
      {
        "command": "goosecode.codeSources.disableCodeSource",
        "title": "Disable CodeSource",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/toggle_on.svg",
          "dark": "resources/dark/toggle_on.svg"
        }
      },
      {
        "command": "goosecode.start",
        "title": "Start",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/toggle_on.svg",
          "dark": "resources/dark/toggle_on.svg"
        }
      },
      {
        "command": "goosecode.stop",
        "title": "Stop",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/toggle_on.svg",
          "dark": "resources/dark/toggle_on.svg"
        }
      },
      {
        "command": "goosecode.restart",
        "title": "Restart",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.regenerateCertificatesAndPassword",
        "title": "Purge Certificates and Regenerate Password",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.snippet",
        "title": "Snippet",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.snippetMinimized",
        "title": "Snippet [Minimized]",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.generate",
        "title": "Generate",
        "category": "GooseCode"
      },
      {
        "command": "goosecode.broadcast",
        "title": "Broadcast for 60s",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/step.svg",
          "dark": "resources/dark/step.svg"
        }
      },
      {
        "command": "goosecode.copyPassword",
        "title": "Copy Connection Password",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/copy.svg",
          "dark": "resources/dark/copy.svg"
        }
      },
      {
        "command": "goosecode.connections.enableLocalhostOnly",
        "title": "Enable Localhost Only",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/toggle_off.svg",
          "dark": "resources/dark/toggle_off.svg"
        }
      },
      {
        "command": "goosecode.connections.disableLocalhostOnly",
        "title": "Disable Localhost Only",
        "category": "GooseCode",
        "icon": {
          "light": "resources/light/toggle_on.svg",
          "dark": "resources/dark/toggle_on.svg"
        }
      },
      {
        "command": "goosecode.changePortForWorkspace",
        "title": "Change Port for This Workspace",
        "category": "GooseCode"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "goosecode.openWalkthrough"
        },
        {
          "command": "goosecode.openSettings"
        },
        {
          "command": "goosecode.appcommand.minimap",
          "when": "goosecode.connected == true"
        },
        {
          "command": "goosecode.appcommand.overlay",
          "when": "goosecode.connected == true"
        },
        {
          "command": "goosecode.codeSources.refresh",
          "when": "false"
        },
        {
          "command": "goosecode.codeSources.enableCodeSource",
          "when": "false"
        },
        {
          "command": "goosecode.codeSources.disableCodeSource",
          "when": "false"
        },
        {
          "command": "goosecode.start",
          "when": "goosecode.started == false"
        },
        {
          "command": "goosecode.stop",
          "when": "goosecode.started == true"
        },
        {
          "command": "goosecode.restart",
          "when": "goosecode.started == true"
        },
        {
          "command": "goosecode.regenerateCertificatesAndPassword"
        },
        {
          "command": "goosecode.snippet",
          "when": "false"
        },
        {
          "command": "goosecode.snippetMinimized",
          "when": "false"
        },
        {
          "command": "goosecode.generate",
          "when": "false"
        },
        {
          "command": "goosecode.copyPassword",
          "when": "goosecode.started == true"
        },
        {
          "command": "goosecode.connections.enableLocalhostOnly",
          "when": "goosecode.started == true"
        },
        {
          "command": "goosecode.connections.disableLocalhostOnly",
          "when": "goosecode.started == true"
        },
        {
          "command": "goosecode.changePortForWorkspace",
          "when": "goosecode.portConflict == true || goosecode.started == true"
        }
      ],
      "view/title": [
        {
          "command": "goosecode.codeSources.refresh",
          "group": "navigation",
          "when": "view == goosecode.codeSources"
        },
        {
          "command": "goosecode.copyPassword",
          "when": "view == goosecode.connection && goosecode.started == true",
          "group": "navigation"
        },
        {
          "command": "goosecode.broadcast",
          "when": "view == goosecode.connection && goosecode.started == true",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "goosecode.stop",
          "when": "view == goosecode.connection && viewItem == enabled",
          "group": "inline"
        },
        {
          "command": "goosecode.start",
          "when": "view == goosecode.connection && viewItem == disabled",
          "group": "inline"
        },
        {
          "command": "goosecode.regenerateCertificatesAndPassword",
          "when": "view == goosecode.connection"
        },
        {
          "command": "goosecode.connections.enableLocalhostOnly",
          "when": "view == goosecode.connection && viewItem == localhostItem && !goosecode.isLocalhostOnly",
          "group": "inline"
        },
        {
          "command": "goosecode.connections.disableLocalhostOnly",
          "when": "view == goosecode.connection && viewItem == localhostItem && goosecode.isLocalhostOnly",
          "group": "inline"
        },
        {
          "command": "goosecode.broadcast",
          "when": "view == goosecode.connection && viewItem == discoverableItem",
          "group": "inline"
        },
        {
          "command": "goosecode.changePortForWorkspace",
          "when": "view == goosecode.connection && viewItem == portConflict",
          "group": "inline"
        },
        {
          "command": "goosecode.codeSources.enableCodeSource",
          "when": "view == goosecode.codeSources && viewItem == codeSource:disabled",
          "group": "inline"
        },
        {
          "command": "goosecode.codeSources.disableCodeSource",
          "when": "view == goosecode.codeSources && viewItem == codeSource:enabled",
          "group": "inline"
        }
      ],
      "editor/context": [
        {
          "submenu": "goosecode.submenu",
          "group": "goosecode"
        }
      ],
      "goosecode.submenu": [
        {
          "when": "editorTextFocus && goosecode.connected == true",
          "command": "goosecode.snippet",
          "group": "goosecode@4"
        },
        {
          "when": "editorTextFocus && goosecode.connected == true",
          "command": "goosecode.snippetMinimized",
          "group": "goosecode@4"
        },
        {
          "when": "editorTextFocus && goosecode.connected == true",
          "command": "goosecode.generate",
          "group": "goosecode@6"
        },
        {
          "when": "goosecode.connected == false && goosecode.started == false && goosecode.initialized == true",
          "command": "goosecode.start",
          "group": "goosecode@9"
        },
        {
          "when": "goosecode.connected == false && goosecode.started == false && goosecode.initialized == false",
          "command": "goosecode.openWalkthrough",
          "group": "goosecode@10"
        },
        {
          "when": "goosecode.connected == false && goosecode.started == true && goosecode.initialized == true",
          "command": "goosecode.openWalkthrough",
          "group": "goosecode@11"
        }
      ]
    },
    "submenus": [
      {
        "id": "goosecode.submenu",
        "label": "GooseCode"
      }
    ],
    "configuration": {
      "title": "GooseCode",
      "properties": {
        "goosecode.startAutomatically": {
          "description": "Start GooseCode Extension server automatically on open.",
          "type": "boolean",
          "default": false
        },
        "goosecode.connections.port": {
          "description": "The port GooseCode will connect to the IDE.",
          "type": "number",
          "default": 60053
        },
        "goosecode.connections.localhostOnly": {
          "description": "Allow only localhost devices to connect.",
          "type": "boolean",
          "default": true
        },
        "goosecode.connections.password": {
          "description": "Password protection to prevent unwanted connections.",
          "type": "string",
          "default": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^2.5.2",
    "@grpc/grpc-js": "^1.13.4",
    "@protobuf-ts/grpc-backend": "^2.11.0",
    "@protobuf-ts/plugin": "^2.11.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/express-ws": "^3.0.1",
    "@types/glob": "^8.1.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/node-forge": "^1.3.11",
    "@types/streamx": "^2.9.5",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.77.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "bonjour-service": "^1.3.0",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "google-protobuf": "^3.21.2",
    "node-forge": "^1.3.1",
    "smol-toml": "^1.3.0",
    "streamx": "^2.22.1",
    "uuid": "^10.0.0",
    "ws": "^8.13.0"
  }
}
